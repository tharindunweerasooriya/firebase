    <activity
        android:id="@+id/ownerLogin"
        android:name="com.teamevox.freshfred.OwnerLogin"
        android:label="activity_owner_login"
        tools:layout="@layout/activity_owner_login" />
    <activity
        android:id="@+id/cart"
        android:name="com.teamevox.freshfred.Cart"
        android:label="activity_cart"
        tools:layout="@layout/activity_cart" />
    <activity
        android:id="@+id/loginRegister2"
        android:name="com.teamevox.freshfred.LoginRegister"
        android:label="activity_login_register"
        tools:layout="@layout/activity_login_register" />

</navigation>